    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"from src.CnnClassifier.constants import *\\n\",\n",
    "    \"from src.CnnClassifier.utils.common import read_yaml,create_directories\"\n",
    "   ],\n",
