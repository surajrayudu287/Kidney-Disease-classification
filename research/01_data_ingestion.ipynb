    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"from dataclasses import dataclass\\n\",\n",
    "    \"from pathlib import Path\\n\",\n",
    "    \"\\n\",\n",
    "    \"\\n\",\n",
    "    \"@dataclass(frozen=True)\\n\",\n",
